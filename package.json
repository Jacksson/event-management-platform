{
  "name": "event-management-platform",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node --env-file=.env dist/main/server.js",
    "dev": "ts-node-dev --project tsconfig.json --env-file=.env --respawn --transpile-only src/bootstrap.ts src/main/server.ts",
    "build": "rm -rf dist/* && tsc --sourceMap false",
    "up:build": "docker-compose up -d --build",
    "up": "docker-compose up -d",
    "test": "npm run build && jest --no-cache --detectOpenHandles --forceExit --coverage",
    "test:watch": "npm run build && jest --forceExit --detectOpenHandles --watchAll",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "pretty-quick": "pretty-quick",
    "pretty-check": "pretty-quick --check",
    "pre-commit": "npm run lint && npm run test",
    "migrate": "sequelize db:migrate"
  },
  "keywords": [],
  "author": "Jackson E. Mosquera R.",
  "license": "ISC",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "express": "^4.19.2",
    "express-validator": "^7.2.0",
    "helmet": "^7.1.0",
    "husky": "^9.1.5",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-extended": "^3.0.7",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.3",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.5",
    "tsyringe": "^4.8.0",
    "winston": "^3.14.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/reflect-metadata": "^0.1.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  },
  "_moduleAliases": {
    "@infrastructure": "dist/src/infrastructure",
    "@domain": "dist/src/domain",
    "@application": "dist/src/application",
    "@interfaces": "dist/src/interfaces",
    "@shared": "dist/src/shared"
  }
}
